// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int longestsubarraySum(vector<int>arr,int k){
    int maxLen=INT_MIN;
    int n=arr.size();
    vector<int>prefix(n);
    prefix[0]=arr[0];
    for(int i=1;i<n;i++){
        prefix[i]=arr[i]+prefix[i-1];
    }
    unordered_map<int,int>mp;
    for(int i=0;i<n;i++){
        if(prefix[i]==k){
       maxLen=max(maxLen,i+1);
        }
        int rem=prefix[i]-k;
        if(mp.find(rem)!=mp.end()){
          maxLen=max(maxLen,i-mp[rem])
        }
       if(mp.find(prefix[i])==mp.end()){
           mp[prefix[i]]=i;
       }
    }
    return maxLen;
}
int main() {
    vector<int>arr={10, 5, 2, 7, 1, -10};
    int k=15;
    int ans=longestsubarraySum(arr,k);
    cout<<ans<<endl;
    return 0;
}
