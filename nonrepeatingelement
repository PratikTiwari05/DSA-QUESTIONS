// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
void nonRepeatingCharacter(string str){
    //check for  uppercase character
    string value="";
    for(int i=0;i<str.size();i++){
        char ch=str[i];
        if(ch>='A' && ch<='Z'){
            value+=ch-'A'+'a';
            
        }
        else{
            value+=ch;
        }
    }
cout<<value<<endl;
    int freq[26]={0};
    for(int i=0;i<value.size();i++){
        char ch=str[i];
        freq[ch-'a']++;
    }
    
    //first non  repeating character
    for(int i=0;i<value.size();i++){
        char ch=str[i];
        if(freq[ch-'a']==1){
            cout<<ch<<endl;
            break;
        }
    }
}
int main() {
  string str="Abbcade";
  nonRepeatingCharacter(str);
    return 0;
}
