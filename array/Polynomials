import java.util.* ;
import java.io.*; 
public class Solution {
    public static int[] multiply(int[] a, int[] b, int n)
    {
         int size=n+n-1;
         int[]ans= new int[size];

         for(int i=0;i<a.length;i++){
             for(int j=0;j<b.length;j++){
                 ans[i+j]+=a[i]*b[j];
             }
         }
         return ans;
    }
}
