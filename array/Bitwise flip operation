import java.util.* ;
import java.io.*; 
public class Solution {
    public static int minimumFlips(int a, int b, int c) {
        // Write your code here.
        int count=0;
        for(int i=0;i<31;i++) {
        int A=(a>>i)&1;
        int B=(b>>i)&1;
        int C=(c>>i)&1;

        if(C==0){
            count+=A+B;
        }
        else{
            if(A==0&&B==0){
                count+=1;
            }
        }
        }
        return count;
    }
}
