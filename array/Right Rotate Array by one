import java.util.* ;
import java.io.*; 


public class Solution {

    static int[] rotateArray(int[] arr, int n) {
       int []ans=new int[n];
       for(int i=0;i<n;i++){
           if(i==0){
               ans[i]=arr[n-1];
           }
           if(i==n-1){
               ans[i]=arr[0];
           }
           ans[i]=arr[i];
       }
       return ans;
    }
}
