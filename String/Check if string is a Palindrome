#include <bits/stdc++.h> 
bool checkPalindrome(string s)
{
    string ans="";

    for(int i=0;i<s.size();i++){
        char ch=s[i];
        if((ch>='A' && ch<='Z') || (ch>='a' && ch<='z') ||(ch>='0' && ch<='9')){

        if(ch>='A' && ch<='Z'){
            ans+=ch-'A'+'a';
        }
        else{
            ans+=ch;
            }
        }
    }

    int i=0;
    int j=ans.size()-1;
    while(i<=j){
        if(ans[i]!=ans[j]){
            return false;
        }
        i++;
        j--;
    }
    return true;
}
