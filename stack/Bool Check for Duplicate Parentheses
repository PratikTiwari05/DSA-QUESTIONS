#include <bits/stdc++.h> 
bool duplicateParanthesis(string &expr)
{
    stack<char>st;
    for(int i=0;i<expr.size();i++){
        char ch=expr[i];
        if(ch!=')'){
            st.push(ch);
        }
        else{
            int count=0;
          while( !st.empty() && st.top()!='('){
             st.pop();
             count++;
          }
          st.pop();
          if(count==0) return true;
        }
    }
    return false;
}
